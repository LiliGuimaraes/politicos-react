    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.7.1",
    "eslint-plugin-react": "^7.3.0",
    "rimraf": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "es6-promise-debounce": "^1.0.1",
